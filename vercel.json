{
  "version": 2,
  "builds": [
    {
      "src": "/index.php",
      "use": "@now/php"
    }
  ],
  "routes": [
    {
      "src": "/extrair-informacoes",
      "dest": "/index.php"
    }
  ],
  "env": {
    "VERCEL_GITHUB_REPO": "seu-usuario/seu-repositorio",
    "VERCEL_GITHUB_COMMIT_REF": "main"
  }
}
